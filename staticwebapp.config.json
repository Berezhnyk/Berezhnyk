{
  "routes": [
    {
      "route": "/api/*",
      "headers": {
        "cache-control": "public, max-age=3600"
      }
    },
    {
      "route": "/fonts/*",
      "headers": {
        "cache-control": "public, max-age=31536000"
      }
    },
    {
      "route": "/images/*",
      "headers": {
        "cache-control": "public, max-age=31536000"
      }
    },
    {
      "route": "/_nuxt/*",
      "headers": {
        "cache-control": "public, max-age=31536000"
      }
    }
  ],
  "mimeTypes": {
    ".woff2": "font/woff2",
    ".webp": "image/webp"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  }
}
